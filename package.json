{
  "name": "nguard",
  "version": "0.4.4",
  "description": "Next.js 16+ session management library with built-in auth() function, callback-based architecture, and zero-config SessionProvider",
  "main": "dist/src/index.js",
  "bin": {
    "nguard-setup": "dist/cli/setup.js",
    "nguard-update": "dist/cli/update.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trxyazilimedu/nguard.git"
  },
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/src/index.js",
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./server": {
      "require": "./dist/src/server.js",
      "import": "./dist/src/server.js",
      "types": "./dist/src/server.d.ts"
    },
    "./client": {
      "require": "./dist/src/client.js",
      "import": "./dist/src/client.js",
      "types": "./dist/src/client.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "tsc --watch",
    "setup": "node --loader ts-node/esm cli/setup.ts",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "next.js",
    "session",
    "authentication",
    "jwt",
    "cookies"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "next": ">=16.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.2.2",
    "next": "^16.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3"
  }
}
